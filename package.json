{
    "name": "fsmat-at",
    "private": true,
    "scripts": {
        "frontend:dev": "yarn workspace frontend dev",
        "frontend:build": "yarn workspace frontend ",
        "frontend:start": "yarn workspace frontend ",
        "cms:dev": "yarn workspace dev ",
        "cms:build": "yarn workspace dev ",
        "cms:start": "yarn workspace dev ",
        "dev": "concurrently \"yarn workspace cms dev\" \"yarn workspace frontend dev\"",
        "build": "yarn cms:build && concurrently \"yarn cms:start && yarn frontend:build\"",
        "start": "concurrently \"yarn cms:start\" \"yarn frontend:start\"",
        "format": "prettier --write ."
    },
    "workspaces": [
        "packages/*"
    ],
    "packageManager": "yarn@4.4.1",
    "devDependencies": {
        "concurrently": "^9.1.2",
        "prettier": "^3.5.3"
    }
}
